<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="create_specificationDic_table" author="Valeriya">
        <createTable tableName="specification_dic">
            <column autoIncrement="true" name="id_specification" type="bigint">
                <constraints primaryKey="true"/>
            </column>

            <column name="nm_specification" type="varchar(250)"/>

        </createTable>
    </changeSet>
</databaseChangeLog>

